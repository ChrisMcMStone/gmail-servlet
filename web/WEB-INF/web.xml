<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>Error</servlet-name>
        <servlet-class>Error</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchContactController</servlet-name>
        <servlet-class>SearchContact</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddContactController</servlet-name>
        <servlet-class>AddContact</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Error</servlet-name>
        <url-pattern>/Error</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchContactController</servlet-name>
        <url-pattern>/SearchContactController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddContactController</servlet-name>
        <url-pattern>/AddContactController</url-pattern>
    </servlet-mapping>
</web-app>